{
  "name": "template-api",
  "version": "0.0.1",
  "description": "",
  "author": "Vodworks",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test,migration}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test,migration}/**/*.ts\" --fix",
    "test": "jest --config ./test/jest.config.js --runInBand",
    "test:watch": "jest --config ./test/jest.config.js --runInBand --watch",
    "test:cov": "jest --config ./test/jest.config.js --runInBand --coverage",
    "test:debug": "node --inspect-brk=0.0.0.0:9229 -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config test/jest.config.js --runInBand",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate -d ormconfig.ts migration/migration -p",
    "migration:create": "npx typeorm-ts-node-commonjs migration:create migration/migration",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d ormconfig.ts",
    "migration:run:test": "npx typeorm-ts-node-commonjs migration:run -d ormconfig.test.ts",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d ormconfig.ts"
  },
  "dependencies": {
    "@fastify/static": "^6.5.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-fastify": "^9.1.2",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/typeorm": "^9.0.0",
    "axios": "^1.6.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "joi": "^17.6.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.7",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.0",
    "@faker-js/faker": "^8.0.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/jest": "29.5.2",
    "@types/node": "^20.10.0",
    "@types/passport-jwt": "^3.0.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-only-error": "^1.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-proto": "^1.122.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.8.3"
  },
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=8.1.2"
  },
  "packageManager": "yarn@1.22.19"
}
