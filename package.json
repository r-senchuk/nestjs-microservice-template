{
  "name": "template-api",
  "version": "0.0.1",
  "description": "",
  "author": "Vodworks",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test,migration}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test,migration}/**/*.ts\" --fix",
    "test": "jest --config ./test/jest.config.js --runInBand",
    "test:watch": "jest --config ./test/jest.config.js --runInBand --watch",
    "test:cov": "jest --config ./test/jest.config.js --runInBand --coverage",
    "test:debug": "node --inspect-brk=0.0.0.0:9229 -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config test/jest.config.js --runInBand",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate -d ormconfig.ts migration/migration -p",
    "migration:create": "npx typeorm-ts-node-commonjs migration:create migration/migration",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d ormconfig.ts",
    "migration:run:test": "npx typeorm-ts-node-commonjs migration:run -d ormconfig.test.ts",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d ormconfig.ts"
  },
  "resolutions": {
    "string-width": "4.2.3"
  },
  "dependencies": {
    "@fastify/static": "^7.0.4",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/microservices": "^9.0.8",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.3.10",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.7.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "joi": "^17.13.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0",
    "kafkajs": "^2.2.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^10.3.10",
    "@types/jest": "29.5.12",
    "@types/node": "^20.14.11",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "eslint": "9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^4.0.0",
    "jest": "29.7.0",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.20",
    "supertest": "^7.0.0",
    "ts-jest": "29.2.2",
    "ts-node": "^10.9.2",
    "ts-proto": "^1.181.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.5.3",
    "typescript-eslint": "^7.16.1"
  },
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=8.1.2"
  },
  "packageManager": "yarn@1.22.19"
}
